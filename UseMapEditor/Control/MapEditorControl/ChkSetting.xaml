<UserControl x:Class="UseMapEditor.Control.MapEditorControl.ChkSetting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UseMapEditor.Control.MapEditorControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <ScrollViewer CanContentScroll="True" SnapsToDevicePixels="True" HorizontalScrollBarVisibility="Visible" PanningMode="VerticalOnly" DataContext="{Binding}">
        <StackPanel x:Name="MainList" CanVerticallyScroll="True" ScrollViewer.CanContentScroll="True">
            <local:ChksettingListItem/>
            <local:ChksettingListItem/>
            <local:ChksettingListItem/>
        </StackPanel>
    </ScrollViewer>
   
</UserControl>
